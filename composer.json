{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.2.0",
        "christophrumpel/missing-livewire-assertions": "^2.8.0",
        "doctrine/dbal": "^3.8.6",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^10.48.16",
        "laravel/pulse": "^1.2.4@beta",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9.0",
        "livewire/livewire": "^3.5.2",
        "maatwebsite/excel": "^3.1.55",
        "openspout/openspout": "^4.24.2",
        "postare/blade-mdi": "^1.1",
        "power-components/livewire-powergrid": "^5.9.0",
        "spatie/laravel-data": "^3.12.0",
        "spatie/laravel-permission": "^6.9",
        "staudenmeir/eloquent-eager-limit": "^1.8.3"
    },
    "require-dev": {
        "defstudio/pest-plugin-laravel-expectations": "^2.2.5",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.16.2",
        "laravel/sail": "^1.30.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^v7.10.0",
        "pestphp/pest": "^2.34.8",
        "pestphp/pest-plugin-livewire": "^v2.1.0",
        "phpunit/phpunit": "^10.5.17",
        "spatie/laravel-ignition": "^2.8.0",
        "spatie/laravel-ray": "^1.37.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "src/Domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test:lint": "pint --test",
        "lint": "pint",
        "test:unit": "pest"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
